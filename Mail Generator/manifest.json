{
    "manifest_version": 3,
    "name": "Dynamic Mail Writer",
    "version": "1.0",
    "description": "Automatically writes dynamic emails based on user input.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  